<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminvcompRaised">

    <TextView
        android:id="@+id/adcraised"
        android:layout_below="@id/adcraisedtgt"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="@color/MidnightBlue"
        android:text="Fund Raised Rs.0"
        android:textSize="18dp"
        android:textAlignment="center"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/adcraisedtgt"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:textColor="@color/MidnightBlue"
        android:textSize="18dp"
        android:textAlignment="center"
        android:textStyle="bold"/>




    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rcadcTotalRaised"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/adcraised"
        />

</RelativeLayout>